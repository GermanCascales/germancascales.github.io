<style>
.video_yt {
    position: relative;
    width: 100%;
    height: 0;
    padding-bottom: 56.25%;
}
.myvideo {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}
iframe.myvideo {
	border: 0;
}
</style>
<script>
var API_PATH = "http://apih.megastar.fm/";
var APP_URL = "static/";

var current_track_id;

$(document).ready(function () {
	$.get(API_PATH + "?method=music.current_track_id", function (a) {
                current_track_id = a.id_track;

    $.getJSON(API_PATH + "?method=music.track_info&id=" + current_track_id, function (a) {
    	$("#lyrics_block").html(a.lyrics);

    	// $("#track_video_yt").html("<div id=\"track_video_yt\" data-video=\"" + a.video_url + "\"></div>");
    	if (a.video_url && a.video_url !== "null" && a.video_url !== "") {
            var d = a.video_url;
            $("#track_video_i").show();
            // alert("videoo " + d);
            $("#track_video_yyt").html("<iframe src=\"http://www.youtube.com/embed/" + d + "\" class=\"myvideo\"></iframe>");
        } else {
            $("#track_video_i").hide();
        }
    });


    });
});
</script>


